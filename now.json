{
    "name": "SocialExperiences",
    "version": 2,
    "builds": [{ "src": "index.js", "use": "@now/node-server"}],
    "routes": [{ "src": "/(.*)", "dest": "/index.js" }],
    "env": {
        "DB_USER": "@socialexperiences-db-user",
        "DB_PASSWORD": "@socialexperiences-db-password", 
        "DB_HOST": "@socialexperiences-db-host",
        "DB_NAME": "@socialexperiences-db-name"
    }
}